    "date-fns": "^2.28.0",
    "firebase": "^9.8.1",
    "formik": "^2.2.9",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "12.1.6",
    "next-connect": "^0.12.2",
    "nprogress": "0.2.0",
    "prisma": "^3.14.0",
    "rc-calendar": "^9.15.11",
