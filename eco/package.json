    "react-dom": "18.1.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "react-paginate": "^8.1.3",
    "react-scroll": "^1.8.7",
    "react-select": "^5.3.2",
    "recharts": "^2.1.9",
