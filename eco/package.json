{
  "name": "itp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "prepare": "husky install",
    "start": "next start",
    "test:all": "tsc && yarn test:lint && yarn test:check",
    "test:check": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "test:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test:lint-fix": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "test:lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "deploy:dev": "vercel",
    "deploy:prod": "vercel --prod",
    "db:init": "yarn prisma init",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:show": "prisma studio"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^7.8.0",
